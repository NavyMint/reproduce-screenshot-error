{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "bin",
    "baseUrl": ".",
    "typeRoots": [],
    "types": [
      "node", "webdriverio/async", "@wdio/jasmine-framework"
    ],
    "lib": [
      "ES2020",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}